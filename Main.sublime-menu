[
    {
        "id": "preferences",
        "children": [
            {
                "id": "package-settings",
                "children": [
                    {
                        "caption": "QuickLineNavigator",
                        "children": [
                            {
                                "caption": "README",
                                "command": "open_file",
                                "args": {
                                    "file": "${packages}/QuickLineNavigator/README.md"
                                }
                            },
                            {
                                "caption": "-"
                            },
                            {
                                "caption": "Settings – Default",
                                "command": "edit_settings",
                                "args": {
                                    "base_file": "${packages}/QuickLineNavigator/Default.sublime-settings",
                                    "default": "{\n\t$0\n}\n"
                                }
                            },
                            {
                                "caption": "Settings – User",
                                "command": "edit_settings",
                                "args": {
                                    "base_file": "${packages}/QuickLineNavigator/Default.sublime-settings",
                                    "user_file": "${packages}/User/QuickLineNavigator.sublime-settings",
                                    "default": "{\n\t$0\n}\n"
                                }
                            },
                            {
                                "caption": "-"
                            },
                            {
                                "caption": "Key Bindings – Default",
                                "command": "edit_settings",
                                "args": {
                                    "base_file": "${packages}/QuickLineNavigator/Default (${platform}).sublime-keymap",
                                    "default": "[\n\t$0\n]\n"
                                }
                            },
                            {
                                "caption": "Key Bindings – User",
                                "command": "edit_settings",
                                "args": {
                                    "base_file": "${packages}/QuickLineNavigator/Default (${platform}).sublime-keymap",
                                    "user_file": "${packages}/User/Default (${platform}).sublime-keymap",
                                    "default": "[\n\t$0\n]\n"
                                }
                            }
                        ]
                    }
                ]
            }
        ]
    }
]
